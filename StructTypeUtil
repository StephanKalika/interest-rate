package com.suhasghorp.loanperf.structtypes

import org.apache.spark.sql.types.{StructType, DataTypes}
 
object StructTypeUtil {
 
  def getFreddieLoansStructType () : StructType = {
    new StructType()
    .add("creditScore",DataTypes.IntegerType)
    .add("firstPaymentDate",DataTypes.DateType)
    .add("firstTimeHomebuyerFlag",DataTypes.StringType)
    .add("maturityDate",DataTypes.DateType)
    .add("msa",DataTypes.IntegerType)
    .add("mip",DataTypes.StringType)
    .add("numberOfUnits",DataTypes.IntegerType)
    .add("occupancyStatus",DataTypes.StringType)
    .add("ocltv",DataTypes.DoubleType)
    .add("dti",DataTypes.StringType)
    .add("originalUPB",DataTypes.DoubleType)
    .add("oltv",DataTypes.IntegerType)
    .add("originalInterestRate",DataTypes.DoubleType)
    .add("channel",DataTypes.StringType)
    .add("prepaymentPenaltyFlag",DataTypes.StringType)
    .add("productType",DataTypes.StringType)
    .add("propertyState",DataTypes.StringType)
    .add("propertyType",DataTypes.StringType)
    .add("postalCode",DataTypes.StringType)
    .add("loanSequenceNumber",DataTypes.StringType)
    .add("loanPurpose",DataTypes.StringType)
    .add("originalLoanTerm",DataTypes.IntegerType)
    .add("numberOfBorrowers",DataTypes.IntegerType)
    .add("sellerName",DataTypes.StringType)
    .add("servicerName",DataTypes.StringType)
    .add("superConfFlag",DataTypes.StringType)
  }
 
  def getFannieLoansStructType() : StructType = {
      new StructType()
      .add("loanSequenceNumber",DataTypes.StringType)
      .add("channel",DataTypes.StringType)
      .add("sellerName",DataTypes.StringType)
      .add("originalInterestRate",DataTypes.DoubleType)
      .add("originalUpb",DataTypes.DoubleType)
      .add("originalLoanTerm",DataTypes.IntegerType)
      .add("originationDate",DataTypes.DateType)
      .add("firstPaymentDate",DataTypes.DateType)
      .add("originalLTV",DataTypes.DoubleType)
      .add("originalCLTV",DataTypes.DoubleType)
      .add("numberOfBorrowers",DataTypes.IntegerType)
      .add("dti",DataTypes.DoubleType)
      .add("creditScore",DataTypes.IntegerType)
      .add("firstTimeHomebuyerIndicator",DataTypes.StringType)
      .add("loanPurpose",DataTypes.StringType)
      .add("propertyType",DataTypes.StringType)
      .add("numberOfUnits",DataTypes.StringType)
      .add("occupancyStatus",DataTypes.StringType)
      .add("propertyState",DataTypes.StringType)
      .add("zipCode",DataTypes.StringType)
      .add("mip",DataTypes.DoubleType)
      .add("productType",DataTypes.StringType)
      .add("coBorrowerCreditScore",DataTypes.IntegerType)
      .add("mortgageInsType",DataTypes.DoubleType)
      .add("reloMortgageInd",DataTypes.StringType)   
  }
 
  
  def getFannieMonthlyObsStructType() : StructType = {
    //   100003615635|03/01/2006||6.75|       |1 |359|359|02/2036|19100|0|N|||||||||||||||||||
    //   532620460986|01/01/2009||6.75|52727.6|34|326|326|03/2036|00000|0|N|||||||||||||||||||N
    new StructType()
    .add("loanSequenceNumber",DataTypes.StringType)
      .add("reportingPeriod",DataTypes.DateType)
      .add("servicerName",DataTypes.StringType)
      .add("currentInterestRate",DataTypes.DoubleType)
        .add("currentUPB",DataTypes.DoubleType)
        .add("loanAge",DataTypes.IntegerType)
        .add("rmm",DataTypes.IntegerType)
      .add("adjustedRMM",DataTypes.IntegerType)
      .add("maturityDate",DataTypes.StringType) //?
      .add("msa",DataTypes.StringType)
     .add("dqStatus",DataTypes.StringType)
      .add("modificationFlag",DataTypes.StringType)
      .add("zeroBalanceCode",DataTypes.StringType)
      .add("zeroBalanceDate",DataTypes.StringType)
      .add("lastInstallDate",DataTypes.DateType)
      .add("foreclosureDate",DataTypes.DateType)
      .add("dispositionDate",DataTypes.DateType)
      .add("foreclosureCosts",DataTypes.DoubleType)
      .add("repairCosts",DataTypes.DoubleType)
      .add("recoveryCosts",DataTypes.DoubleType)
      .add("holdingExpenses",DataTypes.DoubleType)
      .add("taxesForHolding",DataTypes.DoubleType)
      .add("netSaleProceeds",DataTypes.DoubleType)
      .add("creditEnhProceeds",DataTypes.DoubleType)
      .add("repurchaseProceeds",DataTypes.DoubleType)
      .add("foreclosureProceeds",DataTypes.DoubleType)
      .add("nonIntUPB",DataTypes.DoubleType)
      .add("prinForgivenessUPB", DataTypes.DoubleType)
      .add("repurchaseFlag",DataTypes.StringType)
      .add("foreclosureWriteOff",DataTypes.DoubleType)
      .add("servicingActivityInd",DataTypes.StringType)
  }
 
  def getFreddieMonthlyObsStructType() : StructType = {
    new StructType()
    .add("loanSequenceNumber",DataTypes.StringType)
    .add("reportingPeriod",DataTypes.DateType)
    .add("currentUPB",DataTypes.DoubleType)
    .add("dqStatus",DataTypes.StringType)
    .add("loanAge",DataTypes.IntegerType)
    .add("rmm",DataTypes.IntegerType)
    .add("repurchaseFlag",DataTypes.StringType)
    .add("modificationFlag",DataTypes.StringType)
    .add("zeroBalanceCode",DataTypes.StringType)
    .add("zeroBalanceEffectiveDate",DataTypes.DateType)
    .add("currentInterestRate",DataTypes.DoubleType)
    .add("currentDeferredUPB",DataTypes.DoubleType)
    .add("ddlpi",DataTypes.IntegerType)
    .add("miRecoveries",DataTypes.DoubleType)
    .add("netSalesProceeds",DataTypes.StringType)
    .add("nonMiRecoveries",DataTypes.DoubleType)
    .add("expenses",DataTypes.DoubleType)
    .add("legalCosts",DataTypes.DoubleType)
    .add("maintCosts",DataTypes.DoubleType)
    .add("taxesInsurance",DataTypes.DoubleType)
    .add("miscExpenses",DataTypes.DoubleType)
    .add("actualLossCalc",DataTypes.DoubleType)
    .add("modificationCosts",DataTypes.DoubleType)
  }
 
 def getMsaCountyMappingsStructType() : StructType = {
    new StructType()
    .add("cbsaCode",DataTypes.IntegerType)
  .add("msadCode",DataTypes.IntegerType)
  .add("csaCode",DataTypes.IntegerType)
  .add("cbsaName",DataTypes.StringType)
  .add("msaType",DataTypes.StringType)
  .add("msadName",DataTypes.StringType)
  .add("csaName",DataTypes.StringType)
  .add("county",DataTypes.StringType)
  .add("state",DataTypes.StringType)
  .add("stateFips",DataTypes.IntegerType)
  .add("countyFips",DataTypes.IntegerType)
  .add("countyType",DataTypes.StringType)
  .add("stateAbbreviation",DataTypes.StringType)
}
}
